{
    "name": "madebymikkel/subscriptions",
    "description": "Laravel Stripe subscriptions to allow more customizations, if you're not a fan of Stripe's own subscriptions",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "illuminate/database": "~5.8.0|^6.0|^7.0",
        "illuminate/routing": "~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0",
        "nesbot/carbon": "^2.0",
        "stripe/stripe-php": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MadeByMikkel\\Subscriptions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadeByMikkel\\Subscriptions\\SubscriptionServiceProvider"
            ]
        }
    }
}
